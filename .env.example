# ReflexVaultâ„¢ Environment Configuration

# Massa Blockchain Configuration
VITE_MASSA_RPC_URL=https://mainnet.massa.net/api/v2
VITE_MASSA_TESTNET_RPC_URL=https://testnet.massa.net/api/v2
VITE_MASSA_NETWORK=mainnet

# Smart Contract Addresses
VITE_VAULT_MANAGER_ADDRESS=AS12345...
VITE_REFLEX_ENGINE_ADDRESS=AS67890...
VITE_GOVERNANCE_ADDRESS=AS11111...
VITE_REWARD_DISTRIBUTOR_ADDRESS=AS22222...

# API Configuration
VITE_API_BASE_URL=https://api.reflexvault.com/v1
VITE_WS_URL=wss://ws.reflexvault.com/v1

# Feature Flags
VITE_ENABLE_GOVERNANCE=true
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_ECOSYSTEM_VIEW=true
VITE_ENABLE_MOBILE_APP=false

# Analytics and Monitoring
VITE_ANALYTICS_ID=your_analytics_id
VITE_SENTRY_DSN=your_sentry_dsn
VITE_HOTJAR_ID=your_hotjar_id

# Social and Community
VITE_DISCORD_INVITE=https://discord.gg/reflexvault
VITE_TWITTER_URL=https://twitter.com/ReflexVault
VITE_TELEGRAM_URL=https://t.me/reflexvault

# Development Settings
VITE_DEBUG_MODE=false
VITE_MOCK_DATA=false
VITE_LOG_LEVEL=info

# Security Settings
VITE_ENABLE_CSP=true
VITE_ENABLE_HTTPS_ONLY=true
VITE_SESSION_TIMEOUT=3600

# Performance Settings
VITE_CACHE_DURATION=300
VITE_MAX_RETRIES=3
VITE_REQUEST_TIMEOUT=10000

# UI Configuration
VITE_THEME=dark
VITE_DEFAULT_LANGUAGE=en
VITE_ENABLE_ANIMATIONS=true
VITE_ENABLE_SOUND_EFFECTS=false

# Wallet Configuration
VITE_SUPPORTED_WALLETS=massa-station,massa-wallet
VITE_DEFAULT_WALLET=massa-station

# Notification Settings
VITE_ENABLE_PUSH_NOTIFICATIONS=true
VITE_NOTIFICATION_API_KEY=your_notification_key

# Documentation and Help
VITE_DOCS_URL=https://docs.reflexvault.com
VITE_SUPPORT_EMAIL=support@reflexvault.com
VITE_HELP_CENTER_URL=https://help.reflexvault.com

# Legal and Compliance
VITE_TERMS_URL=https://reflexvault.com/terms
VITE_PRIVACY_URL=https://reflexvault.com/privacy
VITE_COOKIE_POLICY_URL=https://reflexvault.com/cookies